<mvc:View
    controllerName="wb.project1.controller.MainView"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.uxap"
    xmlns:m="sap.m"
    xmlns:layout="sap.ui.layout"
>
    <ObjectPageLayout
        id="ObjectPageLayout"
        upperCaseAnchorBar="false"
        sections="{sectionModel>/Sections}"
    >
        <headerTitle>
            <ObjectPageHeader
                id="hid"
                objectTitle="Querys APP "
                class="customTextColor"
            />
        </headerTitle>

        <headerContent>
            <m:ObjectAttribute
                id="g"
                title=""
                text="Lista de Querys "
            />
        </headerContent>

        <sections>
            <ObjectPageSection
                id="sectionTiles"
                titleUppercase="false"
                title="{sectionModel>SectionName}"
            >
                <subSections>
                    <ObjectPageSubSection
                        id="subSectionTiles"
                        titleUppercase="false"
                        title="{sectionModel>subSections/title}"
                    >
                    
                     <m:VBox  id="mkfdlkgfmn"
                    items="{sectionModel>Tiles}" >
                    <m:GenericTile   id="ghjfhfghfdh" 
                        class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                        header="{sectionModel>TileName}"
                        subheader="{sectionModel>QueryString}"
                        press="onTilePress"  
                        url="{sectionModel>QueryString}"  
                    >
                        <m:tileContent>
                            <m:TileContent
                                id="fghfgh"  
                                unit=""
                                footer=""
                            >
                                <m:content>
                                    <m:ImageContent
                                        id="idttyry"  
                                        src="sap-icon://home-share"
                                    />
                                </m:content>
                            </m:TileContent>
                        </m:tileContent>
                    </m:GenericTile>
                </m:VBox>
                    </ObjectPageSubSection>
                </subSections>
            </ObjectPageSection>
        </sections>
    </ObjectPageLayout>

    <m:Page
        id="page"
        title="{i18n>title}"
    >
        <!-- Aquí iría el contenido de la página -->
    </m:Page>
</mvc:View>
